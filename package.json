{
  "name": "serverless-mongodb-local",
  "version": "1.4.0",
  "description": "Serverless MongoDB local plugin",
  "main": "src/index.js",
  "scripts": {
    "test": "npm run lint",
    "test:integration": "NODE_ENV='' jest test/",
    "lint": "eslint src/**/*.js example/**/*.js",
    "start": "cd ./example && sls offline start --noPrependStageInUrl",
    "cmd": "cd ./example && sls mongodb start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bealearts/serverless-mongodb-local.git"
  },
  "license": "MIT",
  "author": {
    "name": "David Beale",
    "url": "http://bealearts.co.uk/"
  },
  "keywords": [
    "serverless",
    "serverless plugin",
    "sls",
    "plugin",
    "mongodb",
    "cosmosdb",
    "documentdb",
    "local"
  ],
  "dependencies": {
    "fs-extra": "^10.0.0",
    "mongodb": "^4.4.1",
    "mongodb-memory-server-core": "^8.4.1"
  },
  "peerDependencies": {
    "serverless": "^2.0.0 || ^3.0.0"
  },
  "devDependencies": {
    "eslint": "^8.11.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.23.4",
    "execa": "^5.1.1",
    "jest": "^26.6.3",
    "node-fetch": "^2.6.7",
    "serverless": "^2.72.3",
    "serverless-mongodb-local": ".",
    "serverless-offline": "^8.5.0"
  }
